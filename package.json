{
  "name": "angular2-datepicker",
  "version": "1.0.0",
  "description": "Angular2 date picker",
  "keywords": [
    "Angular2",
    "date picker"
  ],
  "main": "dist/my-date-picker/index.js",
  "typings": "dist/my-date-picker/index.d.ts",
  "author": "Amit Mittal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akmittal/mydatepicker.git"
  },
  "engines": {
    "node": ">=5.4"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.js --progress",
    "postinstall": "typings install",
    "prepublish": "tsc && npm run build-css",
    "build-css": "node-sass --r src -o dist"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "es6-shim": "~0.35.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.9",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "css-loader": "~0.23.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.16.1",
    "node-sass": "~3.7.0",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.2.0",
    "style-loader": "~0.13.1",
    "ts-loader": "~0.8.2",
    "typescript": "~1.8.10",
    "typings": "~0.8.1",
    "webpack": "~1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-livereload-plugin": "~0.8.1"
  }
}
